{
    "name":"terminal42/dcawizard",
    "description":"dcaWizard extension for Contao Open Source CMS",
    "keywords":["dca", "wizard", "dcawizard", "contao"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao":">=2.3",
        "contao-community-alliance/composer-installer":"*"
    },
    "autoload":{
        "classmap":["system/modules/dcawizard/widgets"]
    },
    "replace": {
        "contao-legacy/dcawizard": "self.version"
    },
    "extra":{
        "contao": {
            "sources":{
                "system/modules/dcawizard": "system/modules/dcawizard"
            }
        }
    }
}